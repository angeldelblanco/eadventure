<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module>
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	<inherits name='ead.tools.gwttools' />
	<inherits name='ead.common.model' />
	<inherits name='ead.engine.enginecore' />
	<inherits name='ead.tools.gwttools' />
	<inherits name='ead.reader.reader' />
	<inherits name='java.util.lib' />
	<inherits name='org.slf4j.slf4j' />
	<inherits name='com.gwtent.GwtEnt' />
	<inherits name='com.google.gwt.logging.Logging' />
	<inherits name='com.google.gwt.xml.XML' />

	<!-- GLAS -->
	<inherits name="es.eucm.glas.tracker.Gwttracker" />

	<!-- Inherit the GIN (guice for gwt) module -->
	<inherits name='com.google.gwt.inject.Inject' />
	<!-- <inherits name='com.google.inject.GuiceTemp'/> -->


	<source path="gdx/html" />
	<entry-point class='ead.engine.core.gdx.html.GwtLauncher' />

	<set-configuration-property name="gdx.assetpath"
		value="core/engine-core/src/main/resources/" />
	<set-configuration-property name="gdx.assetoutputpath"
		value="core/engine-gdx-html/src/main/webapp/" />
	<!-- To be used when compile is done from Eclipse. But you should really 
		compile with maven. -->
	<!-- <set-configuration-property name="gdx.assetpath" value="../engine-core/src/main/resources/" 
		/> -->
</module>