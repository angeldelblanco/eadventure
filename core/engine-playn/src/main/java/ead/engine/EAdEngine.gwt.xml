<module rename-to='eadengine'>
	<inherits name='playn.PlayN' />
	<inherits name='ead.common.model' />
	<inherits name='ead.engine.enginecore' />
	<inherits name='ead.elementfactories' />
	<inherits name='java.util.lib' />
	<inherits name='org.slf4j.slf4j' />
	<inherits name='com.gwtent.GwtEnt' />
	<inherits name='com.google.gwt.logging.Logging' />
	<inherits name='ead.Resources' />
	<inherits name='org.jbox2d.GwtBox2D' />
	<inherits name="com.google.gwt.xml.XML" />

	<inherits name='fr.hd3d.html5.video.HTML5Video' />
	<!-- Optionally, enable gwt-log's advanced logging features: <inherits name="playn.logging.Enhanced" 
		/> -->

	<!-- Inherit the GIN (guice for gwt) module -->
	<inherits name='com.google.gwt.inject.Inject' />
	<!-- <inherits name='com.google.inject.GuiceTemp'/> -->
	
	<!-- GLAS -->
	<inherits name="es.eucm.glas.tracker.Gwttracker" />

	<set-property name="gwt.logging.logLevel" value="FINE" />
	<set-property name="gwt.logging.enabled" value="TRUE" />
	<set-property name="gwt.logging.consoleHandler" value="ENABLED" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />

	<source path='core' />
	<source path='reader' />
	<source path='html' />

	<!-- <public path="../../../../../resources" /> -->
	<entry-point class='ead.engine.html.HtmlEntryPoint' />
</module>
